{
    "name": "highco/sphinx-bundle",
    "description": "Sphinx search in Symfony",
    "keywords": ["sphinx", "search", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/BLEUROY-HIGHCO/SphinxBundle",
    "license": "MIT",
    "authors": [{
        "name": "Jérémy Romey",
        "email": "jeremy@free-agent.fr"
    },{
        "name": "Community contributors",
        "homepage": "https://github.com/BLEUROY-HIGHCO/SphinxBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "^3.0"
    },
    "require-dev": {
        "white-october/pagerfanta-bundle": "^1.0",
        "doctrine/orm": "^2.5"
    },
    "suggests": {
        "white-october/pagerfanta-bundle": "Pagerfanta",
        "doctrine/orm": "Doctrine"
    },
    "autoload": {
        "psr-4": { "Highco\\SphinxBundle\\": "" },
        "classmap": ["sphinxapi.php"]
    }
}
